<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Room:</strong>
    <%= event.room_id %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= event.category %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= event.date %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= event.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= event.end_time %>
  </p>

  <p>
    <strong>Ticket price:</strong>
    <%= event.ticket_price %>
  </p>

  <p>
    <strong>Number of seats left:</strong>
    <%= event.number_of_seats_left %>
  </p>

</div>
